    "ios:prod": "cross-env APP_ENV=PROD expo start --ios --clear",
    "web": "expo start --web",
    "build:android": "cross-env APP_ENV=PROD eas build --platform android",
    "build:ios": "cross-env APP_ENV=PROD eas build --platform ios",
    "build:all": "cross-env APP_ENV=PROD eas build --platform all"
